
<table class="table table-striped">
 <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Listen</th>
    </tr>
  </thead>
  <tbody>
      {{#each tracks}}
    <tr>
      <th scope="row">{{this.name}}</th>
      <td>
          <audio controls>
  <source src="{{this.preview_url}}" type="audio/mp3">
Your browser does not support the audio element.
</audio>
    </tr>
    {{/each}}
  </tbody>
</table>